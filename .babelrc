{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./src/components",
          "styles": "./src/styles",
          "pages": "./src/pages",
          "router": "./src/router",
          "images": "./src/assets/images",
          "icons": "./src/assets/icons",
          "utils": "./src/utils",
          "hooks": "./src/hooks",
          "constants": "./src/constants",
          "store": "./src/store",
          "api": "./src/api",
          "UI": "./src/components/UI",
          "layouts": "./src/layouts",
          "video": "./src/assets/video"
        }
      }
    ]
  ]
}
